<div class="col d-flex justify-content-center">
  <div class="card" style="width: 18rem;">
    <div class="card-body">
      <div class="d-flex justify-content-between">
        <span class="fs-4">
          {{!-- {{#each this.label}}
            {{#if SFW}}
              👔
            {{/if}}
            {{#if dinner table}}
              🍽
            {{/if}}
            {{#if night out}}
              👯
            {{/if}}
            {{#if first date}}
              🍷
            {{/if}}
          {{/each}} --}}
        </span>
        <span class="fs-4">😨😐</span>
      </div>
      <p class="card-text text-center p-2">{{this.cardText}}</p>
      <div class="d-flex justify-content-between">
        <a href="#" class="card-link"><img
            src="/images/pencil.svg"
            alt=""
          /></a>
        <a href="#" class="card-link"><img
            src="/images/trash3.svg"
            alt=""
          /></a>
      </div>
    </div>
  </div>
</div>